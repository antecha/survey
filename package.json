{
  "name": "survey",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "test": "react-scripts test --watchAll=false --coverage",
    "test:ci": "react-scripts test --watchAll=false --env=jsdom --coverage --ci --no-cache --maxWorkers=2",
    "updateSnapshot": "react-scripts test --watchAll=false --coverage --updateSnapshot",
    "flow": "flow",
    "lint": "eslint 'src/**/*.js' --quiet",
    "lint:fix": "npm run lint --fix",
    "build": "react-app-rewired build",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/config/*.js",
      "!src/*.js",
      "!src/**/styles.js",
      "src/**/*.js"
    ],
    "coverageReporters": [
      "text-summary",
      "text",
      "json-summary",
      "lcov"
    ]
  },
  "dependencies": {
    "@antecha/ui-private": "^1.2.4",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.3.5",
    "antd": "^3.26.4",
    "axios": "^0.19.2",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-hook-form": "^5.4.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-babel": "^9.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "flow-bin": "^0.123.0",
    "jest-styled-components": "^7.0.2",
    "prettier": "^2.0.4",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-test-renderer": "^16.12.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
